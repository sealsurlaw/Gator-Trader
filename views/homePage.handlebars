
<!-- body w/ carousel + sidebar -->
<div class="container-fluid">    
    
    <div class="row">

    <!-- carousel column -->
    <div class="col-sm-9" style="background-color:#EBEBEB;">
        
    <!-- Recent Books Carousel -->    
    <h4><center>Recent Books</center></h4>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
    
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1" class="active1"></li>
      <li data-target="#myCarousel" data-slide-to="2" class="active2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <center>
    <div class="carousel-inner">
      
      <div class="item active">
        <div class="item">
          {{#each books}}
            <a href="/item/{{item_id}}"><img src="{{item_image_thumbnail}}" alt="{{item_title}}" style="width:20%"></a>
          {{/each}}
        </div>
      </div>

      <div class="item active1">
        <div class="item">
          {{#each books1}}
            <a href="/item/{{item_id}}"><img src="{{item_image_thumbnail}}" alt="{{item_title}}" style="width:20%"></a>
          {{/each}}
        </div>
      </div>
    
      <div class="item active2">
        <div class="item">
          {{#each books2}}
            <a href="/item/{{item_id}}"><img src="{{item_image_thumbnail}}" alt="{{item_title}}" style="width:20%"></a>
          {{/each}}
        </div>
      </div>
    
    </div>
    </center>


    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
      <span class="sr-only">Next</span>
    </a>
    </div> <!-- End Recent Books Carousel -->  

